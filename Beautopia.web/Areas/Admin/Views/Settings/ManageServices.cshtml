<div class="panel panel-primary">
	<div class="panel-heading">Service Category</div>


	<div id="tabs" style="margin-top: 10px;">
	

		<div class="container-fluid" id="IssueCard">


			<form id="manageService" method="post" novalidate>

				<div class="row row-xs-space mt-1">
					<div class="col-sm-4">
						<Label>
							Category Name En
						</Label>
							<input type="text" name="CategoryNameEn" id="CategoryNameEn" class="form-control inputClass" autocomplete="off" placeholder="Category Name En" />

						
					</div>
					<div class="col-sm-4 mt-1 mt-sm-0">
						<Label>
							Category Name Ar
						</Label>
							<input type="hidden" name="ID" id="ID" value="0" />
							<input type="text" name="CategoryNameAr" id="CategoryNameAr" class="form-control inputClass" autocomplete="off" placeholder="Category Name Ar" />
						
					</div>
					<div class="col-sm-4 mt-1 mt-sm-0" style=" text-align: center;">
						<Label>
							IsActive
						</Label>
							<input type="checkbox" name="IsActiveChecked" id="IsActiveChecked" class="form-control chkClass" />
						
					</div>
				</div>



				<div class="text-right mt-2">
					<button type="submit" class="btn btn-success">Save</button>
				</div>
			</form>

		</div>


		<div id="CardHolderList">
			<div class="row">
				<div class="col-xs-18 col-md-12 ServiceCatereceivedGridDiv" style="margin-top: 22px;">
					<div id="ServiceCatereceivedGrid"></div>

				</div>
			</div>

		</div>

	</div>
</div>

@section scripts {
	<script src="~/admin-custom/js/manage-service.js"></script>
}
